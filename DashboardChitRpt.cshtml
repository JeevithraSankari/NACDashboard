    <script>

        $(function () {
            //To set Current Date -Start

            var d = new Date();

            var month = d.getMonth() + 1;
            var day = d.getDate();

            var output = (('' + day).length < 2 ? '0' : '') + day + '-' +
                         (('' + month).length < 2 ? '0' : '') + month + '-' +
                         d.getFullYear();

            $('#lblcurrentdate').html(output);
            //To get the User Type  - Start
            var UserType = "";
            $.ajax
             ({
                 type: "POST",
                 url: "GetUserType",
                 dataType: "text",
                 success: function (dataaaa) {
                     console.log('USERTYPE : ' + dataaaa);
                     UserType = dataaaa;
                 }
             });

            //For Redirecting to  Dashboard Table - Start
            $('#btnTable').click(function () {
                window.location = ("/Home/DashboardChitTable");

            });

            //For Redirecting to  Dashboard Table - End 
             
            $('#DivChitcoll').click(function () {

                window.location = ("/Home/DailyChitCollection");

            });

            $('#DIVPDCCheque').click(function () {

              window.location = ("/Home/PDCChequeReminder?USERTYPE=" + UserType );
            
            });

            //To get the Name of the Logged in Person - Start

            $.ajax
             ({
                 type: "POST",
                 url: "GetUserName",
                 dataType: "text",
                 success: function (dataaaa) {
                     console.log('USERNAME : ' + dataaaa);
                     $('#lblLoggedInby').html(dataaaa);
                 }
             });

            //To get the Name of the Logged in Person - End

            function AmountWithComma(amt) {
                var x = amt;
                x = x.toString();
                var afterPoint = '';
                if (x.indexOf('.') > 0)
                    afterPoint = x.substring(x.indexOf('.'), x.length);
                x = Math.floor(x);
                x = x.toString();
                var lastThree = x.substring(x.length - 3);
                var otherNumbers = x.substring(0, x.length - 3);
                if (otherNumbers != '')
                    lastThree = ',' + lastThree;
                var AmountSepWithComma = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + lastThree + afterPoint;
                return AmountSepWithComma;
            }
            // High chart for Chit collection start
            $.ajax
             ({
                 type: "POST",
                 url: "DashboardChitCollection",
                 dataType: "json",
                 success: function (dataaaa) {
                     piechartML = new Highcharts.Chart
                   ({
                       chart: {
                           renderTo: 'Chitcontainer',
                           plotBackgroundColor: null,
                           plotBorderWidth: null,
                           plotShadow: false
                       },
                       credits: { enabled: false },

                       exporting: { enabled: false },
                       title: {
                           text: ''
                       },
                       tooltip: {
                           pointFormat: '{series.name}: <b>{point.y} Rs.</b>',
                           percentageDecimals: 1
                       },
                       plotOptions: {
                           pie: {
                               allowPointSelect: true,
                               cursor: 'pointer',
                               dataLabels: {
                                   enabled: true
                               },
                               showInLegend: false
                           }
                       },
                       series: [{
                           type: 'pie',
                           name: "AMOUNT",
                           data: dataaaa.aaDatadtchitcollec
                       }]
                   });
                 }
             });
            // High chart for Chit collection end

            //DashboardPDC starts//
            $.ajax
                    ({
                        type: "POST",
                        url: "PDCChequepend",
                        dataType: "json",
                        success: function (data) {
                            console.log();
                            chartGW = new Highcharts.Chart({
                                chart: {
                                    renderTo: 'DIVPDC',
                                    zoomType: 'xy'
                                },
                                credits: { enabled: false },
                                title: {
                                    text: ''
                                },
                                exporting: {
                                    buttons: {
                                        printButton: {
                                            enabled: false
                                        },
                                        exportButton: {
                                            enabled: false
                                        }
                                    }
                                },
                                exporting: { enabled: false },
                                xAxis: [{
                                    categories: data.aaDataSTATUS1,
                                    labels: {
                                        rotation: -45,
                                        align: 'right',
                                        style: {
                                            fontSize: '10px',
                                            fontWeight: 'bold',
                                            color: (Highcharts.theme && Highcharts.theme.textColor) || 'black',
                                            fontFamily: 'Verdana, sans-serif'
                                        }
                                    }
                                }],
                                yAxis:
                                [{ // Secondary yAxis
                                    title: {
                                        text: 'Pending(Count)',
                                        style: {
                                            fontSize: '15px',
                                            fontWeight: 'bold',
                                            fontFamily: 'Verdana, sans-serif',
                                            color: (Highcharts.theme && Highcharts.theme.textColor) || '#89A54E'
                                        }
                                    },
                                    labels: {
                                        formatter: function () {
                                            return this.value;
                                        },
                                        style: {
                                            fontSize: '10px',
                                            fontWeight: 'bold',
                                            color: (Highcharts.theme && Highcharts.theme.textColor) || '#89A54E',
                                            fontFamily: 'Verdana, sans-serif'
                                        }
                                    },
                                    opposite: true
                                },
                                { // Primary yAxis
                                    labels: {
                                        formatter: function () {
                                            return this.value + ' Rs.';
                                        },
                                        style: {
                                            fontSize: '10px',
                                            fontWeight: 'bold',
                                            color: (Highcharts.theme && Highcharts.theme.textColor) || '#4572A7',
                                            fontFamily: 'Verdana, sans-serif'
                                        }
                                    },
                                    title: {
                                        text: 'Cheque Amount (In Rs.)',
                                        style: {
                                            fontSize: '15px',
                                            fontWeight: 'bold',
                                            fontFamily: 'Verdana, sans-serif',
                                            color: (Highcharts.theme && Highcharts.theme.textColor) || '#4572A7'
                                        }
                                    },
                                    legend:
                                    {
                                        itemStyle:
                                        {
                                            color: 'black',
                                            fontWeight: 'bold',
                                            fontSize: '13px'
                                        },
                                        backgroundColor: '#FFFFFF',
                                        reversed: true
                                    }
                                }],
                                tooltip:
                                         {
                                             formatter: function () {
                                                 name = this.x;

                                                 if (this.series.name == 'CHEQUE AMOUNT') {
                                                     varGrams = ' Rs.';
                                                     return 'AMOUNT, ' + this.x + ': ' + Highcharts.numberFormat(this.y, 0) + varGrams;
                                                 }

                                                 else {
                                                     varGrams = 'COUNT';
                                                     return 'PENDING COUNT, ' + this.x + ': ' + Highcharts.numberFormat(this.y, 0) + varGrams;
                                                 }
                                                 // return '' + this.x + ': ' + Highcharts.numberFormat(this.y, 0) + varGrams;
                                             }
                                         },
                                legend:
                                {
                                    itemStyle:
                                    {
                                        color: 'black',
                                        fontWeight: 'bold',
                                        fontSize: '13px'
                                    },
                                    backgroundColor: '#FFFFFF',
                                    reversed: true
                                },

                                dataLabels:
                                        {
                                            formatter: function () {
                                                return Highcharts.numberFormat(this.y, 0);
                                            },
                                            enabled: true,
                                            color: (Highcharts.theme && Highcharts.theme.dataLabelsColor) || 'White'
                                        },
                                series:
                                [{
                                    name: 'CHEQUE AMOUNT',
                                    color: '#009933',
                                    type: 'column',
                                    yAxis: 1,
                                    data: data.aaDataAMT1
                                },

                                {
                                    name: 'PENDING COUNT',
                                    color: '#800080',
                                    type: 'spline',
                                    data: data.aaDataCOUNT
                                }]
                            });
                        }
                    });

            //PDC ends




        });
    </script>
