    <script>

        $(function () {
            //To set Current Date -Start

            var d = new Date();

            var month = d.getMonth() + 1;
            var day = d.getDate();

            var output = (('' + day).length < 2 ? '0' : '') + day + '-' +
                         (('' + month).length < 2 ? '0' : '') + month + '-' +
                         d.getFullYear();

            //To get the User Type - Start
            var UserType = "";
            $.ajax
             ({
                 type: "POST",
                 url: "GetUserType",
                 dataType: "text",
                 success: function (dataaaa) {
                     console.log('USERTYPE : ' + dataaaa);
                     UserType = dataaaa;
                 }
             });

            $('#lblcurrentdate').html(output);

            //For Redirecting to  Dashboard Table - Start
            $('#btnReport').click(function () {
                window.location = ("/Home/DashboardChitRpt");

            });

            //For Redirecting to  Dashboard Table - End 


            $('#DivChitcoll').click(function () {
                           
                window.location = ("/Home/DailyChitCollection");
                   
            });

            $('#DIVPDCCheque').click(function () {

                window.location = ("/Home/PDCChequeReminder?USERTYPE=" + UserType);
                    
            });

            //To get the Name of the Logged in Person - Start

            $.ajax
             ({
                 type: "POST",
                 url: "GetUserName",
                 dataType: "text",
                 success: function (dataaaa) {
                     console.log('USERNAME : ' + dataaaa);
                     $('#lblLoggedInby').html(dataaaa);
                 }
             });

            //To get the Name of the Logged in Person - End
            //To set Current Date - End
            function AmountWithComma(amt) {
                var x = amt;
                x = x.toString();
                var afterPoint = '';
                if (x.indexOf('.') > 0)
                    afterPoint = x.substring(x.indexOf('.'), x.length);
                x = Math.floor(x);
                x = x.toString();
                var lastThree = x.substring(x.length - 3);
                var otherNumbers = x.substring(0, x.length - 3);
                if (otherNumbers != '')
                    lastThree = ',' + lastThree;
                var AmountSepWithComma = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + lastThree + afterPoint;
                return AmountSepWithComma;
            }

            $.ajax
             ({
                 type: "POST",
                 url: "DashboardChitCollection",
                 dataType: "json",
                 success: function (dataaaa) {
                     console.log('aaDatadtchitcollec :' + dataaaa.aaDatadtchitcollec);

                     var TableBottomBorder = 'style="border-bottom-color:White; border-bottom-style:solid; border-bottom-width:thin;"';
                     var TableStart = '<table width=100%><tr><th ' + TableBottomBorder + '><h5>';
                     var TableEnd = '</table>';
                     var Alignright = 'align="right"';
                     var TableHeading = 'CHIT</h5></th><th ' + TableBottomBorder + '><h5> AMOUNT</h5></th><th ' + TableBottomBorder + '><h5> COUNT</h5></th><th></th></tr>';
                     var FinalTable = '';
                     var TableContent = '';
                     var rowcount = dataaaa.aaDatadtchitcollec.length;

                     for (var ii = 0; ii < dataaaa.aaDatadtchitcollec.length; ii++) {
                         if (ii == dataaaa.aaDatadtchitcollec.length - 1) {
                             TableContent = TableContent + '<tr><td ' + TableBottomBorder + '><strong>' + dataaaa.aaDatadtchitcollec[ii][0] + '</strong></td><td ' + Alignright + ' ' + TableBottomBorder + '><strong>' + AmountWithComma(dataaaa.aaDatadtchitcollec[ii][1]) + '</strong></td><td ' + Alignright + ' ' + TableBottomBorder + '><strong>' + dataaaa.aaDatadtchitcollec[ii][2] + '</strong></td></tr>';
                         }
                         else {
                             if (ii == 0) {
                                 TableContent = TableContent + '<tr><td><strong>' + dataaaa.aaDatadtchitcollec[ii][0] + '</strong></td><td ' + Alignright + '><strong>' + AmountWithComma(dataaaa.aaDatadtchitcollec[ii][1]) + '</strong></td><td ' + Alignright + '><strong>' + dataaaa.aaDatadtchitcollec[ii][2] + '</strong></td><td rowspan="' + rowcount + '" style="padding-left:5px;"><i class="fa fa-chevron-right"></i></td></tr>';
                             }
                             else {
                                 TableContent = TableContent + '<tr><td><strong>' + dataaaa.aaDatadtchitcollec[ii][0] + '</strong></td><td ' + Alignright + '><strong>' + AmountWithComma(dataaaa.aaDatadtchitcollec[ii][1]) + '</strong></td><td ' + Alignright + '><strong>' + dataaaa.aaDatadtchitcollec[ii][2] + '</strong></td></tr>';
                             }
                         }
                     }

                     FinalTable = TableStart + TableHeading + TableContent + TableEnd;
                     $("#divCHITCOLLECTION").html(FinalTable);
                 }
             });
            //PDC REMINDER STARTS//

            $.ajax
             ({
                 type: "POST",
                 url: "DashboardPDC",
                 dataType: "json",
                 success: function (dataaaa) {
                     console.log('aaDataPDC :' + dataaaa.aaDataPDCRemainder);

                     var TableBottomBorder = 'style="border-bottom-color:White; border-bottom-style:solid; border-bottom-width:thin;"';
                     var TableStart = '<table width=100%><tr><th ' + TableBottomBorder + '><h5>';
                     var TableEnd = '</table>';
                     var Alignright = 'align="right"';
                     var TableHeading = 'STATUS</h5></th><th ' + TableBottomBorder + '><h5> CHEQUE AMOUNT</h5></th><th ' + TableBottomBorder + '><h5> COUNT</h5></th><th></th></tr>';
                     var FinalTable = '';
                     var TableContent = '';
                     var rowcount = dataaaa.aaDataPDCRemainder.length;

                     for (var ii = 0; ii < dataaaa.aaDataPDCRemainder.length; ii++) {
                         if (ii == dataaaa.aaDataPDCRemainder.length - 1) {
                             TableContent = TableContent + '<tr><td ' + TableBottomBorder + '><strong>' + dataaaa.aaDataPDCRemainder[ii][0] + '</strong></td><td ' + Alignright + ' ' + TableBottomBorder + '><strong>' + AmountWithComma(dataaaa.aaDataPDCRemainder[ii][1]) + '</strong></td><td ' + Alignright + ' ' + TableBottomBorder + '><strong>' + dataaaa.aaDataPDCRemainder[ii][2] + '</strong></td></tr>';
                         }
                         else {
                             if (ii == 0) {
                                 TableContent = TableContent + '<tr><td><strong>' + dataaaa.aaDataPDCRemainder[ii][0] + '</strong></td><td ' + Alignright + '><strong>' + AmountWithComma(dataaaa.aaDataPDCRemainder[ii][1]) + '</strong></td><td ' + Alignright + '><strong>' + dataaaa.aaDataPDCRemainder[ii][2] + '</strong></td><td rowspan="' + rowcount + '" style="padding-left:5px;"><i class="fa fa-chevron-right"></i></td></tr>';
                             }
                             else {
                                 TableContent = TableContent + '<tr><td><strong>' + dataaaa.aaDataPDCRemainder[ii][0] + '</strong></td><td ' + Alignright + '><strong>' + AmountWithComma(dataaaa.aaDataPDCRemainder[ii][1]) + '</strong></td><td ' + Alignright + '><strong>' + dataaaa.aaDataPDCRemainder[ii][2] + '</strong></td></tr>';
                             }
                         }
                     }

                     FinalTable = TableStart + TableHeading + TableContent + TableEnd;
                     $("#divPDCCHIT").html(FinalTable);
                 }
             });
            //PDC REMAINDER ENDS//
        });
    </script>
