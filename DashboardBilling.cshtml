    <script type="text/javascript">
        $(function () {

            //To set Current Date -Start

            var d = new Date();

            var month = d.getMonth() + 1;
            var day = d.getDate();

            var output = (('' + day).length < 2 ? '0' : '') + day + '-' +
                         (('' + month).length < 2 ? '0' : '') + month + '-' +
                         d.getFullYear();

            $('#lblcurrentdate').html(output);

            //To set Current Date - End

            //To get the Name of the Logged in Person - Start

            $.ajax
             ({
                 type: "POST",
                 url: "GetUserName",
                 dataType: "text",
                 success: function (dataaaa) {
                     console.log('USERNAME : ' + dataaaa);
                     $('#lblLoggedInby').html(dataaaa);
                 }
             });

            //To get the Name of the Logged in Person - End



            //line and stack chart
            var bars = false;
            var lines = true;
            var pie = false;
            $('#btnTable').click(function () {
                window.location = ("/Home/BillingDashboardTable");

            });

            var initializeFlot = function () {


                $.ajax
            ({
                type: "POST",
                url: "dashboardtotsal",
                dataType: "json",
                async: false,
                success: function (dataaa) {

                    var s = dataaa.aaDatadashboardtotsalrpt[0][1] + dataaa.aaDatadashboardtotsalrpt[0][2];
                    var divstart = "<thead> <tr> ";
                    var divcontentstart = "<th></th><th style='color:#22BEEF;'> SALE </th> <th style='color: #ffc100;'> PURCHASE </th> ";
                    var divcontentmiddle = "</tr> </thead>";
                    var divcontent = "<tbody>";
                    var finaloutput = "";
                    var divend = "</tbody>";
                    for (i = 0; i < dataaa.aaDatadashboardtotsalrpt.length; i++) {
                        var divcontentend = "<tr><th>" + dataaa.aaDatadashboardtotsalrpt[i][0] + "</th><td>" + dataaa.aaDatadashboardtotsalrpt[i][1] + "</td><td>" + dataaa.aaDatadashboardtotsalrpt[i][2] + "</td></tr>";
                        finaloutput = finaloutput + divcontentend;
                    }
                    var el1 = $('table.flot-chart1');

                    var htmlcontentcomp = divstart + divcontentstart + divcontentmiddle + divcontent + finaloutput + divend;




                    el1[0].innerHTML = htmlcontentcomp;


                    el1.each(function () {
                        var data = $(this).data();
                        var colors = [];
                        var gridColor = data.tickColor || 'rgba(0,0,0,.1)';

                        $(this).find('thead th:not(:first)').each(function () {
                            colors.push($(this).css('color'));
                        });

                        if (data.type) {
                            bars = data.type.indexOf('bars') != -1;
                            lines = data.type.indexOf('lines') != -1;
                            pie = data.type.indexOf('pie') != -1;
                        }

                        $(this).graphTable({
                            series: 'columns',
                            position: 'replace',
                            colors: colors,
                            width: data.width,
                            height: data.height
                        },
    {
        series: {
            stack: data.stack,
            pie: {
                show: pie,
                innerRadius: data.innerRadius || 0,
                label: {
                    show: data.pieLabel == 'show' ? true : false
                }
            },
            bars: {
                show: bars,
                barWidth: data.barWidth || 0.5,
                fill: data.fill || 1,
                align: 'center'
            },
            lines: {
                show: lines,
                fill: 0.1,
                lineWidth: 3
            },
            shadowSize: 0,
            points: {
                radius: 4
            }
        },
        xaxis: {
            mode: 'categories',
            tickLength: 0,
            font: {
                lineHeight: 24,
                weight: '300',
                color: data.fontColor,
                size: 14
            }
        },
        yaxis: {
            tickColor: gridColor,
            // tickFormatter: function number(x) { var num; if (x >= 1000) { num = (x / 1000) + 'k'; } else { num = x; } return num; },
            max: s, //'3000000',
            font: {
                lineHeight: 13,
                weight: '300',
                color: data.fontColor
            }
        },
        grid: {
            borderWidth: {
                top: 0,
                right: 0,
                bottom: 1,
                left: 1
            },
            borderColor: gridColor,
            margin: 13,
            minBorderMargin: 0,
            labelMargin: 20,
            hoverable: true,
            clickable: true,
            mouseActiveRadius: 6
        },
        legend: { show: data.legend == 'show' ? true : false },
        tooltip: data.toolTip == 'show' ? true : false,
        tooltipOpts: { content: (pie ? '%p.0%, %s' : '<b>%s</b> :  %y') }
    });
                    });
                }
            });



                $.ajax
            ({
                type: "POST",
                url: "dashboardYearComp",
                dataType: "json",
                async: false,
                success: function (dataa) {

                    //var s = dataa.aaDatadashboardtotsalrpt[0][1] + dataa.aaDatadashboardtotsalrpt[0][2];
                    var divstart = "<thead> <tr> ";
                    var divcontentstart = "<th></th><th style='color:#22BEEF;'> AMOUNT - " + dataa.aaDatadashboardytdcomprpt[0][1] + "</th><th style='color:#FF4A43;'>AMOUNT - " + dataa.aaDatadashboardytdcomprpt1[0][1] + "</th><th style='color:#FFC100;'>AMOUNT - " + dataa.aaDatadashboardytdcomprpt2[0][1] + "</th> ";
                    var divcontentmiddle = "</tr> </thead>";
                    var divcontent = "<tbody>";
                    var finaloutput = "";
                    var divend = "</tbody>";
                    var tocheckhighestvalue = 0;
                    for (i = 0; i < dataa.aaDatadashboardytdcomprpt.length; i++) {
                        var divcontentend = "<tr><th>" + dataa.aaDatadashboardytdcomprpt[i][0] + "</th><td>" + dataa.aaDatadashboardytdcomprpt[i][2] + "</td><td>" + dataa.aaDatadashboardytdcomprpt1[i][2] + "</td><td>" + dataa.aaDatadashboardytdcomprpt2[i][2] + "</td></tr>";
                        finaloutput = finaloutput + divcontentend;
                        if (tocheckhighestvalue < dataa.aaDatadashboardytdcomprpt[i][2]) {
                            tocheckhighestvalue = dataa.aaDatadashboardytdcomprpt[i][2]
                        }
                        if (tocheckhighestvalue < dataa.aaDatadashboardytdcomprpt1[i][2]) {
                            tocheckhighestvalue = dataa.aaDatadashboardytdcomprpt1[i][2]
                        }
                        if (tocheckhighestvalue < dataa.aaDatadashboardytdcomprpt2[i][2]) {
                            tocheckhighestvalue = dataa.aaDatadashboardytdcomprpt2[i][2]
                        }


                    }
                    console.log('tocheckhighestvalue : ' + tocheckhighestvalue);
                    var el = $('table.flot-chart');

                    var htmlcontentcomp = divstart + divcontentstart + divcontentmiddle + divcontent + finaloutput + divend;




                    el[0].innerHTML = htmlcontentcomp;


                    el.each(function () {
                        var data = $(this).data();
                        var colors = [];
                        var gridColor = data.tickColor || 'rgba(0,0,0,.1)';

                        $(this).find('thead th:not(:first)').each(function () {
                            colors.push($(this).css('color'));
                        });

                        if (data.type) {
                            bars = data.type.indexOf('bars') != -1;
                            lines = data.type.indexOf('lines') != -1;
                            pie = data.type.indexOf('pie') != -1;
                        }

                        $(this).graphTable({
                            series: 'columns',
                            position: 'replace',
                            colors: colors,
                            width: data.width,
                            height: data.height
                        },
    {
        series: {
            stack: data.stack,
            pie: {
                show: pie,
                innerRadius: data.innerRadius || 0,
                label: {
                    show: data.pieLabel == 'show' ? true : false
                }
            },
            bars: {
                show: bars,
                barWidth: data.barWidth || 0.5,
                fill: data.fill || 1,
                align: 'center'
            },
            lines: {
                show: lines,
                fill: 0.1,
                lineWidth: 3
            },
            shadowSize: 0,
            points: {
                radius: 4
            }
        },
        xaxis: {
            mode: 'categories',
            tickLength: 0,
            font: {
                lineHeight: 24,
                weight: '300',
                color: data.fontColor,
                size: 14
            }
        },
        yaxis: {
            tickColor: gridColor,
            // tickFormatter: function number(x) { var num; if (x >= 1000) { num = (x / 1000) + 'k'; } else { num = x; } return num; },
            max: tocheckhighestvalue + 10000, //'3000000',
            font: {
                lineHeight: 13,
                weight: '300',
                color: data.fontColor
            }
        },
        grid: {
            borderWidth: {
                top: 0,
                right: 0,
                bottom: 1,
                left: 1
            },
            borderColor: gridColor,
            margin: 13,
            minBorderMargin: 0,
            labelMargin: 20,
            hoverable: true,
            clickable: true,
            mouseActiveRadius: 6
        },
        legend: { show: data.legend == 'show' ? true : false },
        tooltip: data.toolTip == 'show' ? true : false,
        tooltipOpts: { content: (pie ? '%p.0%, %s' : '<b>%s</b> :  %y') }
    });
                    });
