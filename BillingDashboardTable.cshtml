    <script>

        $(function () {

            //To set Current Date -Start

            var d = new Date();

            var month = d.getMonth() + 1;
            var day = d.getDate();

            var output = (('' + day).length < 2 ? '0' : '') + day + '-' +
                         (('' + month).length < 2 ? '0' : '') + month + '-' +
                         d.getFullYear();

            $('#lblcurrentdate').html(output);

            //To set Current Date - End

            function AmountWithComma(amt) {
                var x = amt;
                x = x.toString();
                var afterPoint = '';
                if (x.indexOf('.') > 0)
                    afterPoint = x.substring(x.indexOf('.'), x.length);
                x = Math.floor(x);
                x = x.toString();
                var lastThree = x.substring(x.length - 3);
                var otherNumbers = x.substring(0, x.length - 3);
                if (otherNumbers != '')
                    lastThree = ',' + lastThree;
                var AmountSepWithComma = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + lastThree + afterPoint;
                return AmountSepWithComma;
            }

            //To get the Name of the Logged in Person - Start

            $.ajax
             ({
                 type: "POST",
                 url: "GetUserName",
                 dataType: "text",
                 success: function (dataaaa) {
                     console.log('USERNAME : ' + dataaaa);
                     $('#lblLoggedInby').html(dataaaa);
                 }
             });

            //To get the Name of the Logged in Person - End

            //For Redirecting to  Dashboard Table - Start
            $('#btnReport').click(function () {
                window.location = ("/Home/DashboardBilling");

            });
            //For Redirecting to  Dashboard Table - End

            //Daily Sales Vs Target - START
            //var TotalTarget = 0.0;
            //var TotalAchieved = 0.0;
            var singledate = "";
            $.ajax
             ({
                 type: "POST",
                 url: "YDTComparision?SinDate=" + singledate,
                 dataType: "json",
                 success: function (dataaaa) {
                     console.log('YDTComparision :' + dataaaa.aaDataYDTComparision);

                     var TableBottomBorder = 'style="border-bottom-color:White; border-bottom-style:solid; border-bottom-width:thin;"';
                     var TableStart = '<table width=100%><tr><th align="left" ' + TableBottomBorder + '><h5>';
                     var TableEnd = '</table>';
                     var Alignright = 'align="right" ';
                     var TableHeading = 'MONTH</h5></th><th ' + Alignright + TableBottomBorder + '><h5> AMOUNT [' + dataaaa.aaDataYDTComparision[0][1] + ']</h5><th ' + Alignright + TableBottomBorder + '><h5> AMOUNT [' + dataaaa.aaDataYDTComparision1[0][1] + ']</h5></th><th ' + Alignright + TableBottomBorder + '><h5> AMOUNT [' + dataaaa.aaDataYDTComparision2[0][1] + ']</h5></th></tr>';
                     var FinalTable = '';
                     var TableContent = '';
                     var rowcount = dataaaa.aaDataYDTComparision.length;

                     for (var ii = 0; ii < dataaaa.aaDataYDTComparision.length; ii++) {
                         if (ii == dataaaa.aaDataYDTComparision.length - 1) {
                             TableContent = TableContent + '<tr><td ' + TableBottomBorder + '><strong>' + dataaaa.aaDataYDTComparision[ii][0] + '</strong></td><td ' + Alignright + ' ' + TableBottomBorder + '><strong>' + AmountWithComma(dataaaa.aaDataYDTComparision[ii][2]) + '</strong></td><td ' + Alignright + ' ' + TableBottomBorder + '><strong>' + AmountWithComma(dataaaa.aaDataYDTComparision1[ii][2]) + '</strong></td><td ' + Alignright + ' ' + TableBottomBorder + '><strong>' + AmountWithComma(dataaaa.aaDataYDTComparision2[ii][2]) + '</strong></td></tr>';
                         }
                         else {
                             if (ii == 0) {
                                 TableContent = TableContent + '<tr><td><strong>' + dataaaa.aaDataYDTComparision[ii][0] + '</strong></td><td ' + Alignright + '><strong>' + AmountWithComma(dataaaa.aaDataYDTComparision[ii][2]) + '</strong></td><td ' + Alignright + '><strong>' + AmountWithComma(dataaaa.aaDataYDTComparision1[ii][2]) + '</strong></td><td ' + Alignright + '><strong>' + AmountWithComma(dataaaa.aaDataYDTComparision2[ii][2]) + '</strong></td><td rowspan="' + rowcount + '" style="padding-left:5px;"></td></tr>';//<i class="fa fa-chevron-right"></i>
                             }
                             else {
                                 TableContent = TableContent + '<tr><td><strong>' + dataaaa.aaDataYDTComparision[ii][0] + '</strong></td><td ' + Alignright + '><strong>' + AmountWithComma(dataaaa.aaDataYDTComparision[ii][2]) + '</strong></td><td ' + Alignright + '><strong>' + AmountWithComma(dataaaa.aaDataYDTComparision1[ii][2]) + '</strong></td><td ' + Alignright + '><strong>' + AmountWithComma(dataaaa.aaDataYDTComparision2[ii][2]) + '</strong></td></tr>';
                             }
                         }
                     }

                     FinalTable = TableStart + TableHeading + TableContent + TableEnd;
                     $("#divYDTComparision").html(FinalTable);
                 }
             });

            //NARESH CHIT START

            var sdate = "";
            $.ajax
             ({
                 type: "POST",
                 url: "CHITNEW_MTD_YTD?SinDate=" + sdate,
                 dataType: "json",
                 success: function (dataaaa) {
                     console.log('CHITNEW_MTD_YTD :' + dataaaa.aaDataCHITNEW_MTD_YTD);

                     var TableBottomBorder = 'style="border-bottom-color:White; border-bottom-style:solid; border-bottom-width:thin;"';
                     var TableStart = '<table width=100%><tr><th align="left" ' + TableBottomBorder + '><h5>';
                     var TableEnd = '</table>';
                     var Alignright = 'align="right" ';
                     var TableHeading = 'CHIT</h5></th><th ' + Alignright + TableBottomBorder + '><h5> AMOUNT</h5></th><th ' + Alignright + TableBottomBorder + '><h5> NO_OF_CHITS</h5></th></tr>';
                     var FinalTable = '';
